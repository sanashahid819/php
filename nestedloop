<body>

    <table border="1px">
      <?php
        $col = 2;
       $row = 10;
    for($i= 1; $i <= $row; $i++){
      ?>
      <tr>
     <tbody> 
     <?php
    for($j= 1 ; $j <= $col; $j++){
        ?>
          <td></td>
      <?php
    }
    ?>
    </tr>
    <?php
}
?>
 </tbody>
</table>
    
